{
	"name": "corp_webapp",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "npx tailwindcss -c ./tailwind.config.cjs -i ./src/input.css -o ./src/output.css --watch & vite --host & nodemon ./api/server.cjs",
		"start_production": "(rm -rf ./dist && npx tailwindcss -c ./tailwind.config.cjs -i ./src/input.css -o ./src/output.css && vite build --base ./ ) && (node ./api/server.cjs & http-server --cors -s -o -c 1 ./dist)",
		"curltime": "curl -w '@curl-format.txt' -o /dev/null -s "
	},
	"_how_to_use_scripts": {
		"curl_time_example": "npm run curltime -- 'http://google.com'"
	},
	"dependencies": {
		"@emotion/react": "^11.9.3",
		"@emotion/styled": "^11.9.3",
		"@mui/icons-material": "^5.8.4",
		"@mui/material": "^5.9.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"formidable": "^2.0.1",
		"mysql": "^2.18.1",
		"nodemailer": "^6.7.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.3.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@vitejs/plugin-react": "^2.0.0",
		"tailwindcss": "^3.1.6",
		"vite": "^3.0.0"
	}
}
